{
    "IdentityBasedPolicies": [
      {
        "PolicyName": "BasicUserPolicy",
        "PolicyArn": "arn:aws:iam::policy/BasicUserPolicy",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:List*"
              ],
              "Resource": "*"
            }
          ]
        }
      },
      {
        "PolicyName": "AdvancedUserPolicy",
        "PolicyArn": "arn:aws:iam::policy/AdvancedUserPolicy",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:List*",
                "s3:DeleteObject"
              ],
              "Resource": "*"
            }
          ]
        }
      },
      {
        "PolicyName": "CreateUserPolicy",
        "PolicyArn": "arn:aws:iam::policy/CreateUserPolicy",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "iam:CreateUser",
                "iam:AttachUserPolicy"
              ],
              "Resource": "*"
            }
          ]
        }
      },
      {
        "PolicyName": "AdminAccess",
        "PolicyArn": "arn:aws:iam::policy/AdminAccess",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": "*",
              "Resource": "*"
            }
          ]
        }
      }
    ],
    "ResourceBasedPolicies": [
      {
          "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                  {
                      "Effect": "Allow",
                      "Principal": {
                          "ID": "arn:aws:iam::123456789012:group/BasicUsers"
                      },
                      "Action": [
                          "s3:GetObject",
                          "s3:ListBucket"
                      ],
                      "Resource": [
                          "arn:aws:s3:: 123456789012:company-files",
                          "arn:aws:s3:: 123456789012:company-files/*"
                      ]
                  },
                  {
                      "Effect": "Allow",
                      "Principal": {
                          "ID": "arn:aws:iam::role/SuperUserRole"
                      },
                      "Action": [
                          "s3:GetObject",
                          "s3:PutObject",
                          "s3:DeleteObject",
                          "s3:ListBucket"
                      ],
                      "Resource": [
                          "arn:aws:s3:: 123456789012:company-files",
                          "arn:aws:s3:: 123456789012:company-files/*"
                      ]
                  }
              ]
          }
      },
      {
          "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                  {
                      "Effect": "Allow",
                      "Principal": {
                          "ID": "arn:aws:iam::123456789012:group/BasicUsers"
                      },
                      "Action": [
                          "s3:GetObject",
                          "s3:ListBucket",
                          "s3:PutObject"
                      ],
                      "Resource": [
                          "arn:aws:s3:: 123456789012:user-files",
                          "arn:aws:s3:: 123456789012:user-files/*"
                      ]
                  },
                  {
                      "Effect": "Allow",
                      "Principal": {
                          "ID": "arn:aws:iam::role/SuperUserRole"
                      },
                      "Action": [
                          "s3:GetObject",
                          "s3:PutObject",
                          "s3:DeleteObject",
                          "s3:ListBucket"
                      ],
                      "Resource": [
                          "arn:aws:s3:: 123456789012:user-files",
                          "arn:aws:s3:: 123456789012:user-files/*"
                      ]
                  }
              ]
          }
      }
  ]
  }
  