{
    "IdentityBasedPolicies": [
      {
        "PolicyName": "BasicUserPolicy",
        "PolicyArn": "custom_id_policy/BasicUserPolicy",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:List*"
              ],
              "Resource": "*"
            }
          ]
        }
      },
      {
        "PolicyName": "AdvancedUserPolicy",
        "PolicyArn": "custom_id_policy/AdvancedUserPolicy",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:List*",
                "s3:DeleteObject"
              ],
              "Resource": "*"
            }
          ]
        }
      },
      {
        "PolicyName": "CreateUserPolicy",
        "PolicyArn": "custom_id_policy/CreateUserPolicy",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "iam:CreateUser",
                "iam:AttachUserPolicy"
              ],
              "Resource": "*"
            }
          ]
        }
      },
      {
        "PolicyName": "AdminAccess",
        "PolicyArn": "custom_id_policy/AdminAccess",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": "*",
              "Resource": "*"
            }
          ]
        }
      }
    ],
    "ResourceBasedPolicies": [
      {
          "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                  {
                      "Effect": "Allow",
                      "Principal": {
                          "ID": "custom_id___custom__:group/BasicUsers"
                      },
                      "Action": [
                          "s3:GetObject",
                          "s3:ListBucket"
                      ],
                      "Resource": [
                          "_custom_s3_id_:company-files",
                          "_custom_s3_id_:company-files/*"
                      ]
                  },
                  {
                      "Effect": "Allow",
                      "Principal": {
                          "ID": "custom_id_role/SuperUserRole"
                      },
                      "Action": [
                          "s3:GetObject",
                          "s3:PutObject",
                          "s3:DeleteObject",
                          "s3:ListBucket"
                      ],
                      "Resource": [
                          "_custom_s3_id_:company-files",
                          "_custom_s3_id_:company-files/*"
                      ]
                  }
              ]
          }
      },
      {
          "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                  {
                      "Effect": "Allow",
                      "Principal": {
                          "ID": "custom_id___custom__:group/BasicUsers"
                      },
                      "Action": [
                          "s3:GetObject",
                          "s3:ListBucket",
                          "s3:PutObject"
                      ],
                      "Resource": [
                          "_custom_s3_id_:user-files",
                          "_custom_s3_id_:user-files/*"
                      ]
                  },
                  {
                      "Effect": "Allow",
                      "Principal": {
                          "ID": "custom_id_role/SuperUserRole"
                      },
                      "Action": [
                          "s3:GetObject",
                          "s3:PutObject",
                          "s3:DeleteObject",
                          "s3:ListBucket"
                      ],
                      "Resource": [
                          "_custom_s3_id_:user-files",
                          "_custom_s3_id_:user-files/*"
                      ]
                  }
              ]
          }
      }
  ]
  }
  