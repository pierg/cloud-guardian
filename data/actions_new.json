{
  "permissions": {
    "moveData": {
      "description": "Authorizes an entity to initiate data movement between datastores.",
      "source": {
        "categories": ["Entity"],
        "types": ["User", "Role", "Service"]
      },
      "aws_action": "s3:CopyObject"
    },
    "belongsTo": {
      "description": "Authorizes an entity to access data that belongs to another entity.",
      "source": {
        "categories": ["Entity"],
        "types": ["User", "Role", "Service"]
      },
      "target": {
        "categories": ["Entity"],
        "types": ["User", "Role", "Service"]
      },
      "aws_action": "s3:GetObject"
    }
}
