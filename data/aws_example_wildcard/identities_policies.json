{
  "IdentityBasedPolicies": [
    {
      "PolicyName": "BasicUserAccess",
      "PolicyArn": "arn:aws:iam::123456789012:policy/BasicUserAccess",
      "PolicyDocument": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": [
              "s3:GetObject",
              "s3:List*",
              "s3:Put*"
            ],
            "Resource": "*"
          }
        ]
      }
    },
    {
      "PolicyName": "AdminAccess",
      "PolicyArn": "arn:aws:iam::aws:policy/AdminAccess",
      "PolicyDocument": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "*",
            "Resource": "*"
          }
        ]
      }
    }
  ]
}
